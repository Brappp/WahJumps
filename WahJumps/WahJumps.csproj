<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  
  <Import Project="Dalamud.Plugin.Bootstrap.targets" />

  <PropertyGroup>
    <Version>1.0.0.2</Version>
    <Description>WahJumps</Description>
    <PackageProjectUrl>https://github.com/Brappp/WahJumps</PackageProjectUrl>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <IsPackable>false</IsPackable>
    
    <!-- Specify the target framework -->
    <TargetFramework>net8.0</TargetFramework> <!-- You can use net6.0 if your project needs it -->
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\Data\wah.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="33.0.1" />
  </ItemGroup>

</Project>
